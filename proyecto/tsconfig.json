{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "jsx": "react-jsx",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noEmit": false,
    "paths": {
      "*": ["./proyecto/src/*"]
    }
  },
  "include": ["src/**/*", "server.ts"],
  "exclude": ["node_modules"]
}